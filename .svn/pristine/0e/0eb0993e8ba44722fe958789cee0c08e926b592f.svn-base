<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

	<!-- 系统参数  <bean id="settingService" class="com.asiainfo.nontaxteam.service.Impl.SettingServiceImpl" autowire="byType"></bean>-->
	
	<!-- 按钮功能 -->
	<bean id="tAuthorityFuncService" class="com.asiainfo.authority.spring.service.impl.TAuthorityFuncServiceImpl" autowire="byType"></bean>
	<!-- 按钮管理 -->
	<bean id="tAuthorityButtonService" class="com.asiainfo.authority.spring.service.impl.TAuthorityButtonServiceImpl" autowire="byType"></bean>
	
	<!-- 角色按钮挂接关系 -->
	<bean id="tAuthorityRbrelationService" class="com.asiainfo.authority.spring.service.impl.TAuthorityRbrelationServiceImpl" autowire="byType"></bean>
	
	<!-- 模块管理 -->
	<bean id="tAuthorityModuleService" class="com.asiainfo.authority.spring.service.impl.TAuthorityModuleServiceImpl" autowire="byType"></bean>
	<!-- 模块导出 -->
	<bean id="tAuthorityModuleService_EXP" class="com.asiainfo.authority.spring.service.exp.TAuthorityModuleServiceImpl_EXP" autowire="byType"></bean>
	
	<!-- 级别管理 -->
	<bean id="tAuthorityLevelService" class="com.asiainfo.authority.spring.service.impl.TAuthorityLevelServiceImpl" autowire="byType"></bean>
	<!-- 角色管理 -->
	<bean id="tAuthorityRoleService" class="com.asiainfo.authority.spring.service.impl.TAuthorityRoleServiceImpl" autowire="byType"></bean>
	<!-- 角色模块挂接关系 -->
	<bean id="tAuthorityRmrelationService" class="com.asiainfo.authority.spring.service.impl.TAuthorityRmrelationServiceImpl" autowire="byType"></bean>
	<!-- 用户管理 -->
	<bean id="tAuthorityUserService" class="com.asiainfo.authority.spring.service.impl.TAuthorityUserServiceImpl" autowire="byType"></bean>
	<!-- 用户角色挂接关系 -->
	<bean id="tAuthorityUrrelationService" class="com.asiainfo.authority.spring.service.impl.TAuthorityUrrelationServiceImpl" autowire="byType"></bean>
	<!-- 机构管理 -->
	<bean id="tAuthorityInstService" class="com.asiainfo.authority.spring.service.impl.TAuthorityInstServiceImpl" autowire="byType"></bean>
</beans>
